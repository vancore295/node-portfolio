{
  "name": "node-portfolio",
  "version": "0.0.0",
  "engines": {
    "node": ">= 6.10.1",
    "npm": ">= 5.5.1"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prestart": "tsc -p server && ng build",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"ng serve --proxy-config --open --host 0.0.0.0\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build -aot -prod && tsc -p server && node dist/server/app.js\"",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/animations": "^7.1.4",
    "@angular/cdk": "^7.2.0",
    "@angular/cli": "^7.1.4",
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/compiler-cli": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/forms": "^7.1.4",
    "@angular/http": "^7.1.4",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/router": "^7.1.4",
    "@types/express": "^4.16.0",
    "@types/jasmine": "^3.3.5",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.18",
    "@types/socket.io": "^2.1.2",
    "angular-font-awesome": "^3.1.2",
    "body-parser": "^1.18.3",
    "bootstrap": "4.2.1",
    "core-js": "^2.6.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "mongoose": "^5.4.0",
    "morgan": "^1.9.1",
    "popper.js": "^1.14.6",
    "rxjs": "^6.3.3",
    "socket.io": "^2.2.0",
    "tslib": "^1.9.0",
    "typescript": "~3.1.1",
    "zone.js": "^0.8.26",
    "rxjs-compat": "^6.0.0-rc.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "^7.1.4",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.1.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "nodemon": "^1.18.9",
    "protractor": "^5.4.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "~3.1.1"
  }
}
