        "jasmine-core": "^2.6.2",
        "jasmine-spec-reporter": "^4.1.0",
        "karma": "^1.7.0",
        "karma-chrome-launcher": "^2.1.1",
        "karma-cli": "^1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "^1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "nodemon": "^1.11.0",
        "protractor": "^5.1.2",
        "ts-node": "^3.0.4",
        "tslint": "^5.3.2",
        "typescript": "^2.5.3"
    }
