{
  "name": "node-portfolio",
  "version": "0.0.0",
  "engines": {
    "node": ">= 8.15.0",
    "npm": ">= 6.4.0"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "postinstall": "tsc -p server && ng build",
    "prestart": "tsc -p server && ng build",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"ng serve --proxy-config --open --host 0.0.0.0\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "node dist/server/app.js",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
