{
    "name": "node-portfolio",
    "version": "0.0.0",
    "engines": {
        "node": ">= 6.10.1",
        "npm": ">= 5.5.1"
    },
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "postinstall": "tsc -p server && ng build -aot -prod",
        "start": "node dist/server/app.js",
        "predev": "tsc -p server",
        "dev": "concurrently \"mongod\" \"ng serve -pc proxy.conf.json --open --host 0.0.0.0\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
        "prod": "concurrently \"mongod\" \"ng build -aot -prod && tsc -p server && node dist/server/app.js\"",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^5.0.0",
        "@angular/cdk": "^5.0.4",
        "@angular/cli": "^1.6.4",
        "@angular/common": "^5.0.0",
        "@angular/compiler": "^5.0.0",
        "@angular/compiler-cli": "^5.0.0",
        "@angular/core": "^5.0.0",
        "@angular/forms": "^5.0.0",
        "@angular/http": "^5.0.0",
        "@angular/material": "^5.0.4",
        "@angular/platform-browser": "^5.0.0",
        "@angular/platform-browser-dynamic": "^5.0.0",
        "@angular/router": "^5.0.0",
        "@types/express": "^4.11.0",
        "@types/jasmine": "^2.5.54",
        "@types/jasminewd2": "^2.0.2",
        "@types/node": "^6.0.88",
        "@types/socket.io": "^1.4.31",
        "angular-font-awesome": "^3.1.2",
        "body-parser": "^1.17.2",
        "bootstrap": "4.0.0-beta.2",
        "core-js": "^2.4.1",
        "dotenv": "^4.0.0",
        "express": "^4.16.2",
        "font-awesome": "^4.7.0",
        "jquery": "^3.2.1",
        "mongoose": "^4.11.8",
        "morgan": "^1.8.2",
        "popper.js": "^1.12.3",
        "rxjs": "^5.5.2",
        "socket.io": "^2.0.4",
        "typescript": "^2.5.3",
        "zone.js": "^0.8.17"
    },
    "devDependencies": {
        "@angular/cli": "^1.6.4",
